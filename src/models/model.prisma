generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Disease {
    id                 Int    @id @default(autoincrement())
    diseaseName        String @db.VarChar(80)
    diseaseDescription String @db.Text

    @@map("disease")
}
